<h2>Available drones</h2>

<section id="drones-list">
    {{#each dronesArr}}
        <p>Name: {{this.name}}</p>
        <p>Propellers: {{this.propellers}}</p>
        <p>Speed: {{this.maxSpeed}}</p>
        <a href="/drones/{{_id}}/edit" >Edit</a>
        <form action="/drones/{{_id}}/delete" method="POST">
            <button>Delete</button>
        </form>
    {{/each}}
</section>